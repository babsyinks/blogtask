<%- include("partials/header") %>
<div class="wrapperAdmin">
<div class="wrapInput signIn adminSignIn">
    <h1>Admin Sign In</h1>
  
  <div class = 'center'>
  <form action="/admin" method="POST" class="adminForm">
  <label for = "signIn_username">UserName:</label> <input type="text" name="username" placeholder="Enter Registered Email"/>
  <br>
  <label for = "signIn_password">Password:</label> <input type="password" name="password" placeholder="Enter Password"/>
  <br>
  <input type="submit" value = "Log In"/>
  </form>
  </div>
  </div>

<p class="center validation"><%= adminValidator %></p>
</div>
<%- include("partials/footer") %>